<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.kosmo.jobkorea.lms.dao.LSMBoardDao">
   
<!-- 강사들이 올린 리스트 조회 -->
	<select id="selectLsmBoardList" resultType="kr.kosmo.jobkorea.lsm.model.taskInfoModel">
	
			SELECT
				 task_seq
				,lec_seq
				,task_nm
				,enr_user
				,enr_date
				,upd_user
				,upd_date
			FROM tb_task

	</select>


</mapper>